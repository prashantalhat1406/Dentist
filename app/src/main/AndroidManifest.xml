<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prashant.dentist" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_dentist"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".homePatient"
            android:label="@string/title_activity_home_patient" >
        </activity>
        <activity
            android:name=".addNewPatient"
            android:label="@string/title_activity_add_new_patient" >
        </activity>
        <activity
            android:name=".viewPatientPage"
            android:label="@string/title_activity_view_patient_page" >
        </activity>
        <activity
            android:name=".addNewAppointment"
            android:label="@string/title_activity_add_new_appointment" >
        </activity>
        <activity
            android:name=".viewAppiontmentPage"
            android:label="@string/title_activity_view_appiontment_page" >
        </activity>
        <activity
            android:name=".editPatientPage"
            android:label="@string/title_activity_edit_patient_page" >
        </activity>
        <activity
            android:name=".editAppointment"
            android:label="@string/title_activity_edit_appointment" >
        </activity>
    </application>

</manifest>
